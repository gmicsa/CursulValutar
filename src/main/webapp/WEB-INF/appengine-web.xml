<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://appengine.google.com/ns/1.0 http://googleappengine.googlecode.com/svn/branches/1.2.1/java/docs/appengine-web.xsd">
    <application>cursul-valutar</application>
    <version>1</version>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties"/>
    </system-properties>
    
    <threadsafe>true</threadsafe>
    
    <static-files>
        <!-- include JS configuration file -->
        <include path="/systemjs.config.js" />
        <!-- include JS script files from npm modules -->
        <include path="/node_modules/**.js" />
        <include path="/node_modules/**.js.map" />
        <!-- include typescript files and transpiled files from exchange service component folder-->
        <include path="/exchangeservice/**" />

        <!-- include other resources: images, partial htmls and locales -->
        <include path="/html/**" />
        <include path="/img/**" />
        <include path="/locales/**" />
        <include path="/fonts/**" />
        <!-- include CSS files -->
        <include path="/node_modules/bootstrap/dist/css/**" />
        <include path="/css/**.css" />
        <!-- include bootstrap fonts -->
        <include path="/node_modules/bootstrap/dist/fonts/glyphicons-halflings-regular.woff2" />
        <include path="/node_modules/bootstrap/dist/fonts/glyphicons-halflings-regular.woff" />
        <include path="/node_modules/bootstrap/dist/fonts/glyphicons-halflings-regular.ttf" />

    </static-files>

</appengine-web-app>
